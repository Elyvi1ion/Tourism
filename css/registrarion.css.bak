*{
	font-family: 'Montserrat Medium';
	box-sizing: border-box;
}


a{
	transition: .3s;
}

header a:hover{
	color: grey;
}



#popup-bg{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top:0;
  background-color: rgba(0,0,0,0.8);
  transition: 0.3s;
  

}
#popup{
  position: absolute;
  width: 400px;
  height: 450px;
  left: 50%;
  top: 50%;
  margin-left:-200px;
  margin-top: -225px; 
 


}


#popup ol {
 padding: 0;
}




.prespect{

perspective: 800px;

}

#popup-bg #popup form{

  position: absolute;
  box-sizing: border-box;
  width: 400px;
  height: 450px;
  background-color: white;
  border-radius: 5px;
  padding: 40px;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  z-index: 2;
  transition: 0.5s;

}

#popup-bg #popup form input {
    display: block;
    width: 100%;
    padding: 0 20px;
    margin-bottom: 10px;
    background: white;
    line-height: 35px;
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}




#popup-bg #popup p1 {
  text-align: center;
    display: block;
    width: 100%;
    padding: 0 20px;
    margin-bottom: 10px;
    background: white;
    line-height: 35px;
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}




#popup-bg #popup a.link2 {
    
    color: black;
    text-decoration: none;
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}










#popup-bg #popup a.link2 {
    display: block;
    width: 100%;
    padding: 0 20px;
    margin-bottom: 10px;
    background: black;
    line-height: 35px;
    color: #ffffff;
    text-decoration: none;
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}
#popup-bg #popup a {
    display: block;
    width: 100%;
    padding: 0 20px;
    margin-bottom: 10px;
    background: white;
    line-height: 35px;
    font-size: 20px;
    color: #33a1ff;
    text-decoration: none;
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}
#popup-bg #popup  button {
    display: block;
    width: 100%;
    padding: 0 20px;
    margin-bottom: 10px;
    background: black;
    /* line-height: 35px; */
    /* border-width: 0; */
    /* border-radius: 20px; */
    font-family: 'Montserrat Light';
}
#popup-bg #popup  button p {
  font-size: 15px;
  color: white;
    font-family: 'Montserrat Light';
}

#popup-bg #popup form:nth-child(2){
  z-index: 1;
  transform: rotateY(-180deg);
}

.hide{
  opacity: 0;
  pointer-events: none;
}

#popup-bg #popup form.roll{
  transform: rotateY(180deg);
  z-index: 1;
}

#popup-bg #popup form.reverse-roll{
  transform: rotateY(0deg);
  z-index: 2;
}




































